package JavaSelfTest;

//subclass: inherits from another class
//superclass: class being inherited from
//keyword: 'extends'

class InheritMethods {
    protected String brand = "Toyota";
    public void color() {
        System.out.println(" is red");
    }
}

class Car extends InheritMethods {
    private String modelName = "Suzuki";

    public static void main(String[] args) {
        Car carObj = new Car();
        carObj.color();

        System.out.println(carObj.brand + " " + carObj.modelName);
    }
}